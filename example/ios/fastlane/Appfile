    team_id ENV['TEAM_ID']
    for_lane :build_fad_ios_alpha do
        app_identifier 'com.bartosz_nowacki.example.alpha'
    end

    for_lane :build_fad_ios_beta do
        app_identifier 'com.bartosz_nowacki.example.beta'
    end

    for_lane :build_ios_production do
        app_identifier 'com.bartosz_nowacki.example'
    end
end